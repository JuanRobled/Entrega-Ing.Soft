      <Button fx:id="btnConfirm" layoutX="219.0" layoutY="328.0" mnemonicParsing="false" onAction="#confirmService" style="-fx-background-color: #29539A;" text="Confirmar" textFill="WHITE" />
      <Button fx:id="btnLeave" layoutX="314.0" layoutY="328.0" mnemonicParsing="false" onAction="#leave" prefHeight="25.0" prefWidth="69.0" style="-fx-background-color: #212327;" text="Salir" textFill="WHITE" />
