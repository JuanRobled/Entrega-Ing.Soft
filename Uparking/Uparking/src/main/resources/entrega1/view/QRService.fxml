      <Label layoutX="198.0" layoutY="57.0" text="Código de Confirmación">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblDate" alignment="CENTER" layoutX="226.0" layoutY="251.0" prefHeight="19.0" prefWidth="150.0" />
