<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #4BA8C4;" xmlns="http://javafx.com/javafx/13" xmlns:fx="http://javafx.com/fxml/1" fx:controller="entrega2.co.uparking.solid.clean.aplicacion.agregados.QRController">
   <children>
